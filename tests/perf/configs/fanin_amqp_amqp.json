{
  "name": "topic-fanin-amqp-amqp",
  "description": "Config-driven topic fan-in: AMQP publishers to AMQP subscribers.",
  "pattern": "fanin",
  "flow": "amqp-amqp",
  "topic": "perf/topic/config/fanin/amqp-amqp/{run_id}",
  "qos": 1,
  "publishers": 200,
  "messages_per_publisher": 100,
  "publish_interval": "100ms",
  "subscribers": 200,
  "wildcard_subscribers": 20,
  "wildcard_patterns": [
    "{topic}",
    "{base}/#"
  ],
  "drain_timeout": "45s",
  "payload_bytes": 4096
}
