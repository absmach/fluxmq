{
  "name": "topic-fanout-mqtt-amqp",
  "description": "Config-driven topic fan-out: MQTT publishers to AMQP subscribers across many topics.",
  "pattern": "fanout",
  "flow": "mqtt-amqp",
  "topic": "perf/topic/config/fanout/mqtt-amqp/{run_id}",
  "qos": 1,
  "topic_count": 48,
  "publishers": 300,
  "messages_per_publisher": 200,
  "publish_interval": "20ms",
  "subscribers": 100,
  "wildcard_subscribers": 24,
  "wildcard_patterns": [
    "{base}/t/+",
    "{base}/#"
  ],
  "drain_timeout": "45s",
  "payload_bytes": 256
}
